<html lang="fa" dir="rtl" style="--primary01:rgba(108, 95, 252, 0.1); --primary02:rgba(108, 95, 252, 0.2); --primary03:rgba(108, 95, 252, 0.3); --primary06:rgba(108, 95, 252, 0.6); --primary09:rgba(108, 95, 252, 0.9); --primary005:rgba(108, 95, 252, 0.05);">
<head>
    <partial name="PartAdminLayout/_StylePartAdminLayout" />
</head>

<body class="app sidebar-mini rtl light-mode">
    <div class="horizontalMenucontainer">

        <!-- GLOBAL-LOADER -->
        <div id="global-loader" style="display: none;">
            <img src="/Template/images/loader.svg" class="loader-img" alt="Loader">
        </div>
        <!-- /GLOBAL-LOADER -->
        <!-- PAGE -->
        <div class="page">
            <div class="page-main is-expanded">
                <partial name="PartAdminLayout/_HeaderPartAdminLayout" />
                <partial name="PartAdminLayout/_MenuPartAdminLayout" />
                <!--app-content open-->
                <div class="main-content app-content mt-0">
                    <div class="side-app">
                        <!-- CONTAINER -->
                        <div class="main-container container-fluid">
                            @RenderBody()
                        </div>
                        <!-- CONTAINER CLOSED -->
                    </div>
                </div>
                <!--app-content close-->
            </div>
            <partial name="PartAdminLayout/OutherPartAdminLayout" />
            <partial name="PartAdminLayout/_FooterPartAdminLayout" />
        </div>

        <!-- BACK-TO-TOP -->
        <a href="#top" id="back-to-top"><i class="fa fa-angle-up"></i></a>

        <partial name="PartAdminLayout/_ScriptPartAdminLayout" />

        @await RenderSectionAsync("Scripts", required: false)


        <svg id="SvgjsSvg1001" width="2" height="0" xmlns="http://www.w3.org/2000/svg" version="1.1" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:svgjs="http://svgjs.com/svgjs" style="overflow: hidden; top: -100%; left: -100%; position: absolute; opacity: 0;"><defs id="SvgjsDefs1002"></defs><polyline id="SvgjsPolyline1003" points="0,0"></polyline><path id="SvgjsPath1004" d="M0 0 "></path></svg>
    </div>
    <div class="jvectormap-tip"></div>
</body>
</html>
